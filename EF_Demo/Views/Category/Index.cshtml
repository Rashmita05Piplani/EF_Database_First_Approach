@model List<EF_Demo.Models.Category>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<h2>Categories</h2>
<table class="table">
    <tr>
        <th>CategoryId</th>
        <th>Category Name</th>
    </tr>
    @foreach (var item in Model)
    {
    <tr>
        <td>@item.CategoryId</td>
        <td>@item.CategoryIdName</td>
    </tr>
    }
</table>
